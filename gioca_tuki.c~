#include "game_model.h"
#include <stdio.h>



direzione gioca_tuki(giocatore p, oggetto **lab){
  static int turno = 0;
  static direzione ld;
  /* avanza turno */
  turno+=1;

  if(turno==1)
    {
      ld = SINISTRA;
    }
  if(turno==9)
    {
      ld = SU;
    }
  if(turno==24)
    {
      ld = SINISTRA;
    }
   if(turno==31)
    {
      ld = SU;
    }
   if(turno==37)
    {
      ld = GIU;
    }
  return ld;
  
}
