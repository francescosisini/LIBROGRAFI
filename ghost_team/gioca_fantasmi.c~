#include "game_model.h"
#include <stdio.h>



direzione gioca_blinky(giocatore p, oggetto **lab)
{
  static int turno = 0;
  direzione ld;
  /* avanza turno */
  turno+=1;
  
  if(turno == 1)
    {
      ld=SU;
      return ld;
    }
   if(turno > 6 && turno <20)
    {
      ld=GIU;
      return ld;
    }
  ld=DESTRA;
  return ld;
}

direzione gioca_inky(giocatore p, oggetto **lab)
{
  direzione ld;
  ld=GIU;
  return ld;
}

direzione gioca_pinky(giocatore p, oggetto **lab)
{
  direzione ld;
  ld=GIU;
  return ld;
}

direzione gioca_clyde(giocatore p, oggetto **lab)
{
  direzione ld;
  ld=GIU;
  return ld;
}
